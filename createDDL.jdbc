CREATE TABLE sc_tags (id INTEGER NOT NULL, bId INTEGER NOT NULL, tag VARCHAR(255), PRIMARY KEY (id))
CREATE TABLE sc_watched (wId INTEGER NOT NULL, uId INTEGER NOT NULL, watched INTEGER NOT NULL, PRIMARY KEY (wId))
CREATE TABLE sc_users (uId INTEGER NOT NULL, email VARCHAR(255) NOT NULL, homepage VARCHAR(255), username VARCHAR(255), uContent TEXT(64000), uDatetime DATETIME NOT NULL, uIp VARCHAR(255), name VARCHAR(255), uStatus TINYINT(1) default 0 NOT NULL, password VARCHAR(255) NOT NULL, isFlagged TINYINT(1) default 0 NOT NULL, isAdmin TINYINT(1) default 0 NOT NULL, uModified DATETIME NOT NULL, activation_key VARCHAR(255), PRIMARY KEY (uId))
CREATE TABLE sc_tagmaps (id INTEGER NOT NULL, fromTag VARCHAR(255), toTag VARCHAR(255), PRIMARY KEY (id))
CREATE TABLE sc_bookmarks (bId INTEGER NOT NULL, bModified DATETIME NOT NULL, bTitle TEXT(64000), uId INTEGER NOT NULL, bAddress TEXT(64000) NOT NULL, bStatus TINYINT(1) default 0 NOT NULL, bDescription TEXT(64000), bHash VARCHAR(255) NOT NULL, bDatetime DATETIME NOT NULL, bFlagCount SMALLINT, bIp VARCHAR(255), bFlaggedBy TEXT(64000), PRIMARY KEY (bId))
CREATE TABLE oer_feeds (id BIGINT NOT NULL, url VARCHAR(255), last_import BIGINT, feed_type VARCHAR(255), PRIMARY KEY (id))
